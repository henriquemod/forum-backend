{
  "name": "pet-wise-qa-backend",
  "module": "src/main/index.ts",
  "type": "module",
  "scripts": {
    "start": "bun dist/main/index.js",
    "dev": "bun --watch src/main/index.ts",
    "build": "tsc --build tsconfig.prod.json && tsc-alias -p tsconfig.prod.json",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test:unit": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit:watch": "jest --passWithNoTests --no-cache --runInBand --watch",
    "test:coverage": "jest --passWithNoTests --no-cache --runInBand --coverage ",
    "test:integration": "jest --config jest.integration.config.ts --passWithNoTests --no-cache --runInBand",
    "prepare": "husky",
    "postinstall": "husky install",
    "seed:mongo": "bun src/infra/db/mongodb/seeds/index.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@faker-js/faker": "8.4.1",
    "@rocketseat/eslint-config": "2.2.2",
    "@shelf/jest-mongodb": "4.2.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/bcrypt": "5.0.2",
    "@types/bun": "latest",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jsonwebtoken": "9.0.6",
    "@types/ramda": "0.30.0",
    "@types/supertest": "6.0.2",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "9.0.8",
    "dotenv": "^16.4.5",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-prettier": "5.1.3",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "mongodb-memory-server": "9.1.8",
    "pretty-quick": "^4.0.0",
    "supertest": "7.0.0",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "tsc-alias": "^1.8.10",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "typescript": "5.3.3"
  },
  "dependencies": {
    "bcrypt": "5.1.1",
    "bullmq": "^5.7.14",
    "cors": "2.8.5",
    "express": "4.19.2",
    "ioredis": "^5.4.1",
    "jsonwebtoken": "9.0.2",
    "mailgun.js": "10.2.1",
    "mongodb": "6.5.0",
    "mongoose": "8.3.0",
    "node-mailjet": "6.0.5",
    "openai": "4.47.1",
    "ramda": "0.30.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "uuid": "9.0.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
