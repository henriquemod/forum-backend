{
  "name": "pet-wise-qa-backend",
  "module": "src/main/index.ts",
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "bun src/main/index.ts",
    "build": "tsc --build",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts",
    "test:unit": "jest --passWithNoTests --no-cache --runInBand",
    "test:unit:watch": "jest --passWithNoTests --no-cache --runInBand --watch",
    "test:coverage": "jest --passWithNoTests --no-cache --runInBand --coverage ",
    "prepare": "husky",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.2",
    "@commitlint/config-conventional": "^19.2.2",
    "@rocketseat/eslint-config": "^2.2.2",
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "peerDependencies": {
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.5.0",
    "mongoose": "^8.3.0"
  }
}
